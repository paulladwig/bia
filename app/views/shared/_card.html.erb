<%if data[:restaurant_reference] %>
  <%= link_to(restaurants_path(restaurant_reference: restaurant_reference), method: :post) do  %>
    <%= render 'shared/default_card', data: data %>
  <% end %>
<%else%>
  <%= link_to(data[:restaurant]) do  %>
    <%= render 'shared/default_card', data: data %>
  <% end %>
<%end%>



